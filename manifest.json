{
	"manifest_version": 2,
	
	"name": "Send to Transmission (No Questions Asked)",
	"description": "Simply send torrent files to remote Transmission server within few clicks.",
	"version": "13.3.8.1",
	
	"permissions": ["contextMenus", "tabs", "http://*/", "https://*/"],
	
	"background": {
		"scripts": ["lib/jquery-1.7.1.min.js",
                "script/bencode.js",
                "script/torrent.js",
                "script/transmission.js",
                "script/settings.js",
                "script/download.js",
                "script/background.js"]
	},
	"options_page": "options.html",
	"icons": {
		"16": "images/icon16.png",
		"32": "images/icon32.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	}
}
